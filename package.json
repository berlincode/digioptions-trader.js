{
  "name": "digioptions-trader.js",
  "version": "0.1.3",
  "description": "javascript trading tools for the ethereum based digital option exchange digioptions.com",
  "homepage": "https://www.digioptions.com",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Ulf Bartel", 
    "email": "elastic.code@gmail.com"
  },
  "scripts": {
    "serve": "http-server . -a 127.0.0.1 -p 8888 -c-1"
  },
  "files": [
    "config.js",
    "COPYING",
    "css",
    "img/digioptions.png",
    "main.js",
    "market.js",
    "README.md",
    "trading-browser.html",
    "trading-node.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlincode/digioptions-trader.js.git"
  },
  "keywords": [
    "option exchange",
    "smart contract",
    "ethereum"
  ],
  "node_deb": {
    "init": "auto",
    "maintainer": "mail@digioptions.com",
    "install_strategy": "copy",
    "entrypoints": {
      "daemon": "trading-node.js 8100 8101 127.0.0.1"
    },
    "user": "digioptions-trader",
    "group": "digioptions-trader"
  },
  "dependencies": {
    "basic-auth": "^1.1.0",
    "bootstrap": "^3.3.7",
    "digioptions-contracts.js": "git+https://github.com/berlincode/digioptions-contracts.js.git",
    "digioptions-tools.js": "git+https://github.com/berlincode/digioptions-tools.js.git",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-html-parser": "^1.0.3",
    "requirejs": "^2.3.3",
    "web3": "^0.19.0",
    "websocket.io": "0.2.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "http-server": "^0.10.0",
    "jshint": "2.9.4"
  }
}
