{
  "name": "digioptions-trader.js",
  "version": "0.4.0",
  "description": "javascript trading tools for the ethereum based digital option exchange digioptions.com",
  "homepage": "https://www.digioptions.com",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Ulf Bartel",
    "email": "elastic.code@gmail.com"
  },
  "scripts": {
    "serve": "http-server . -a 127.0.0.1 -p 8888 -c-1",
    "eslint": "eslint ./*.js ./js/ ./test/*.js ./*.html"
  },
  "files": [
    "js/web3.js",
    "js/utils.js",
    "js/config.js",
    "LICENSE",
    "img/digioptions.png",
    "img/github_ribbon.png",
    "js/main.js",
    "js/market.js",
    "README.md",
    "trading-browser.html",
    "trading-node.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlincode/digioptions-trader.js.git"
  },
  "keywords": [
    "option exchange",
    "smart contract",
    "ethereum"
  ],
  "node_deb": {
    "init": "auto",
    "maintainer": "elastic.code@gmail.com",
    "install_strategy": "copy",
    "entrypoints": {
      "daemon": "trading-node.js --port=8100 --host==127.0.0.1"
    },
    "user": "digioptions-trader",
    "group": "digioptions-trader"
  },
  "dependencies": {
    "basic-auth": "^1.1.0",
    "bootstrap": "^3.3.7",
    "factsigner": "git+https://github.com/berlincode/factsigner",
    "digioptions-contracts.js": "git+https://github.com/berlincode/digioptions-contracts.js.git",
    "digioptions-tools.js": "git+https://github.com/berlincode/digioptions-tools.js.git",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "jsonwebtoken": "^7.4.1",
    "minimist": "^1.2.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-html-parser": "^2.0.2",
    "requirejs": "^2.3.3",
    "web3": "1.0.0-beta.31",
    "websocket": "1.0.25",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "http-server": "^0.10.0",
    "eslint": "4.18.2",
    "eslint-plugin-html": "4.0.2"
  }
}
